@model IEnumerable<FMS_Backend.FMSModels.FuelLocation>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Locname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Geocode)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Locname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Geocode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Locid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Locid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Locid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
